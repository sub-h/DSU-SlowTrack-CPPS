int number()
{
        int n;
        printf("Enter the number of integers to be added\n");
        scanf("%d",&n);
        return n;
}
void input(int n,int arr[n])
{       
        printf("Enter the integers to be added\n");
        for(int i=0;i<n;i++)
        {
                scanf("%d",&arr[i]);
        }
}
int compute(int n,nt arr[n])
{
         int sum=0;
         for(int i=0;i<n;i++)
         {
                 sum+=arr[i];
         }                
}
